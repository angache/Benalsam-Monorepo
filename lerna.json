{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "version": "0.0.0",
  "npmClient": "npm",
  "command": {
    "bootstrap": {
      "hoist": true,
      "nohoist": [
        "**/react-native",
        "**/expo",
        "**/@expo/**",
        "**/react-native-*"
      ]
    },
    "publish": {
      "conventionalCommits": true,
      "message": "chore(release): publish"
    }
  },
  "packages": [
    "packages/*"
  ]
}
